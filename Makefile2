# Makefile for Project 1 of 15-441
CC = gcc
CFLAGS = -g -Wall
LDFLAGS = -lpthread

all: lisod

helper.o: helper.c helper.h
	$(CC) $(CFLAGS) -c helper.c

wrapper.o: wrapper.c wrapper.h
	$(CC) $(CFLAGS) -c wrapper.c

lisod.o: lisod.c wrapper.h helper.h
	$(CC) $(CFLAGS) -c lisod.c

lisod: helper.o wrapper.o lisod.o

clean:
	rm -f *~ *.o lisod *.tar *.zip *.gzip *.bzip *.gz

test: lisod
	